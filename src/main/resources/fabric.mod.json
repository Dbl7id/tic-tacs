{
  "schemaVersion": 1,
  "id": "tick_tacs",
  "version": "${version}",
  "name": "Tick-TACS",
  "description": "Optimizing Minecraft's chunk generation code to reduce impact on server tick rate",
  "authors": ["Gegy"],
  "license": "LGPLv3",
  "environment": "*",
  "entrypoints": {
    "main": []
  },
  "mixins": ["tic_tacs.mixins.json"],
  "accessWidener": "tic_tacs.accesswidener",
  "depends": {
    "fabricloader": ">=0.9.1",
    "fabric": "*",
    "minecraft": "1.16.*"
  },
  "custom": {
    "lithium:options": {
      "mixin.world.chunk_access": false,
      "mixin.world.chunk_ticking": false,
      "mixin.alloc.chunk_ticking": false
    }
  }
}
